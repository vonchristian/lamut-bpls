            <div class="info-box-content">
              <span class="info-box-text">OWNED BY WOMEN</span>
              <span class="info-box-number"><%= Business.owned_by_women.count %><small></small></span>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
        <!-- /.col -->
        <div class="col-md-3 col-sm-6 col-xs-12">
          <div class="info-box">
            <span class="info-box-icon bg-green"><i class="fa fa-google-plus"></i></span>

            <div class="info-box-content">
              <span class="info-box-text">NEW BUSINESSES</span>
              <span class="info-box-number"><%= Business.new_business.count %></span>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
        <div class="col-md-3 col-sm-6 col-xs-12">
