<%= simple_form_for @permit_application_date_range, url: :bplo_section_settings_permit_application_date_ranges do |f| %>
  <%= f.error_notification %>
  <%=f.input :start_date, as: :string, input_html: { class: "datepicker" }%>
  <%=f.input :end_date, as: :string, input_html: { class: "datepicker" }%>

  <%= f.submit 'Save', class: "btn btn-primary", data: { disable_with: "Saving..."} %>
<% end %>
