                  <div class="col-md-1">
                  </div>
                  <div class="col-md-7">
                    <%= fee.name %>
                  </div>
                  <div class="col-md-3">
                    <span class="pull-right"><%=number_to_currency fee.amount %></span>
                  </div>
                </div>
                <br>
              <% end %>
            <% end %>
              <div class="row">
                <div class="col-md-1">
                </div>
                <div class="col-md-7">
