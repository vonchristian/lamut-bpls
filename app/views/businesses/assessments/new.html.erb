                     <%= d.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= d.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                    <%=f.simple_fields_for :credit_amounts do |c|  %>
                      <%=c.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.documentary_stamp_fees.for_current_year}, as: :hidden  %>
                      <%=c.input :account_id, input_html: { value: Fees::DocumentaryStampFee.new.credit_account.id }, as: :hidden  %>
                    <%= c.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= c.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                  <% end %>

                  <!-- Sealing Fee -->
                  <% if @business.total_sealing_fees > 0 %>
                    <%=f.simple_fields_for :debit_amounts do |d|  %>
                      <%=d.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.sealing_fees.for_current_year }, as: :hidden  %>
                      <%=d.input :account_id, input_html: {value: Fees::SealingFee.new.debit_account.id}, as: :hidden %>
                     <%= d.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= d.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                    <%=f.simple_fields_for :credit_amounts do |c|  %>
                      <%=c.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.sealing_fees.for_current_year}, as: :hidden  %>
                      <%=c.input :account_id, input_html: { value: Fees::SealingFee.new.credit_account.id }, as: :hidden  %>
                    <%= c.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= c.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                  <% end %>

                  <!-- Surcharges -->
                  <% if @business.total_surcharges > 0 %>
                    <%=f.simple_fields_for :debit_amounts do |d|  %>
                      <%=d.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_surcharges }, as: :hidden  %>
                      <%=d.input :account_id, input_html: {value: Businesses::Surcharge.new.debit_account.id}, as: :hidden %>
                     <%= d.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= d.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                    <%=f.simple_fields_for :credit_amounts do |c|  %>
                      <%=c.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_surcharges}, as: :hidden  %>
                      <%=c.input :account_id, input_html: { value: Businesses::Surcharge.new.credit_account.id }, as: :hidden  %>
                    <%= c.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= c.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                  <% end %>
                  <!-- Interests -->
                  <% if @business.total_interests > 0 %>
                    <%=f.simple_fields_for :debit_amounts do |d|  %>
                      <%=d.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_interests }, as: :hidden  %>
                      <%=d.input :account_id, input_html: {value: Businesses::LateRegistrationInterest.new.debit_account.id},  as: :hidden %>
                     <%= d.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= d.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                    <%=f.simple_fields_for :credit_amounts do |c|  %>
                      <%=c.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_interests }, as: :hidden  %>
                      <%=c.input :account_id, input_html: { value: Businesses::LateRegistrationInterest.new.credit_account.id }, as: :hidden  %>
                      <%= c.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= c.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                  <% end %>

                  <!-- Business Tax -->
                  <% if @business.total_business_taxes > 0 && !@business.cooperative? && !@business.business_tax_exempted? %>
                    <%=f.simple_fields_for :debit_amounts do |d|  %>
                      <% if @business.annually? %>
                        <%=d.input :amount, label: "Business Tax", input_html: {autocomplete: "off", class: "input-lg", value: @business.total_business_taxes, id: 'business-tax-assessment-debit' }, as: :hidden  %>
                      <% else %>
                        <%=d.input :amount, label: "Business Tax", input_html: {autocomplete: "off", class: "input-lg", value: @business.total_business_taxes, id: 'business-tax-assessment-debit' }  %>
                      <% end %>
                      <%=d.input :account_id, input_html: {value: Accounting::Account.find_by_name("Accounts Receivable - Business Tax").id}, as: :hidden %>
                       <%= d.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= d.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                    <%=f.simple_fields_for :credit_amounts do |c|  %>
                      <%=c.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_business_taxes, id: 'business-tax-assessment-credit' }, as: :hidden  %>
                      <%=c.input :account_id, input_html: { value: Accounting::Account.find_by_name("Business Tax").id }, as: :hidden %>
                    <%= c.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= c.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                  <% end %>
                  <!-- Cockpit Operation Fees -->
                  <% if @business.total_cockpit_operation_fees > 0 %>
                    <%=f.simple_fields_for :debit_amounts do |d|  %>
                      <%=d.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_cockpit_operation_fees }, as: :hidden  %>
                      <%=d.input :account_id, input_html: {value: Accounting::Account.find_by_name("Accounts Receivable - Cockpit Operation Fees").id},  as: :hidden %>
                      <%= d.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= d.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                    <%=f.simple_fields_for :credit_amounts do |c|  %>
                      <%=c.input :amount, input_html: {autocomplete: "off", class: "input-lg", value: @business.total_cockpit_operation_fees }, as: :hidden  %>
                      <%=c.input :account_id, input_html: { value: Accounting::Account.find_by_name("Cockpit Operation Fees").id }, as: :hidden  %>
                      <%= c.input :commercial_document_id, input_html: { value: @business.id }, as: :hidden %>
                      <%= c.input :commercial_document_type, input_html: { value: @business.class.to_s }, as: :hidden %>

                    <% end %>
                  <% end %>

                  <%= f.input :reference_number, as: :hidden %>
                  <%= f.input :entry_date, label: "Assessment Date", as: :string, input_html: { class: "datepicker", default: Date.today } %>
                  <%= f.input :description, input_html: { value: "Assessment of fees and taxes" }, as: :hidden %>
                  <div class="box-footer">
                    <%= f.submit "Save Assessment", class: "btn btn-primary btn-flat pull-right", data: { disable_with: "Saving Assessment..."} %>
                  </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
